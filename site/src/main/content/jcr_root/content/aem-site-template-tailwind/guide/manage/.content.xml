<?xml version="1.0" encoding="UTF-8" ?>
<jcr:root
  xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
  xmlns:cq="http://www.day.com/jcr/cq/1.0"
  xmlns:jcr="http://www.jcp.org/jcr/1.0"
  xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
  jcr:primaryType="cq:Page"
>
    <jcr:content
    cq:template="/conf/aem-site-template-tailwind/settings/wcm/templates/page-content"
    jcr:primaryType="cq:PageContent"
    jcr:title="Manage"
    sling:resourceType="core/wcm/components/page/v3/page"
  >
        <root
      jcr:primaryType="nt:unstructured"
      sling:resourceType="core/wcm/components/container/v1/container"
    >
            <main
        jcr:primaryType="nt:unstructured"
        sling:resourceType="core/wcm/components/container/v1/container"
        layout="responsiveGrid"
      >
                <content
          cq:styleIds="[aem-Grid--max-width]"
          jcr:primaryType="nt:unstructured"
          sling:resourceType="core/wcm/components/container/v1/container"
          layout="responsiveGrid"
        >
                    <breadcrumb
            cq:styleIds="[aem-Grid--max-width]"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="core/wcm/components/breadcrumb/v3/breadcrumb"
            disableShadowing="false"
            hideCurrent="false"
            showHidden="false"
            startLevel="2"
          />
                    <title
            cq:styleIds="[aem-Grid--max-width]"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="core/wcm/components/title/v3/title"
            type="h1"
          />
                    <container
            jcr:primaryType="nt:unstructured"
            sling:resourceType="core/wcm/components/container/v1/container"
          >
                        <text
              jcr:primaryType="nt:unstructured"
              sling:resourceType="core/wcm/components/text/v2/text"
              text="&lt;p>AEM gives you the power and flexibility to build any type of site you can imagine. This guide will help you understand how to work with assets like photos and videos, as well as site elements such as logos and content. You'll also learn how to work with existing pages, like the homepage, and create new pages from scratch, even taking you right through the publishing process, getting your site up and running in a matter of minutes.&lt;/p>&#xd;&#xa;"
              textIsRich="true"
            />
                        <button
              cq:styleIds="[button--primary]"
              jcr:primaryType="nt:unstructured"
              jcr:title="Keep Reading"
              sling:resourceType="core/wcm/components/button/v2/button"
              linkTarget="_self"
              linkURL="/content/aem-site-template-tailwind/guide/manage/assets"
            />
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                jcr:primaryType="nt:unstructured"
                offset="0"
                width="8"
              />
                            <phone
                jcr:primaryType="nt:unstructured"
                offset="0"
                width="12"
              />
                        </cq:responsive>
                    </container>
                    <list
            jcr:primaryType="nt:unstructured"
            sling:resourceType="core/wcm/components/list/v3/list"
            childDepth="1"
            displayItemAsTeaser="false"
            linkItems="true"
            listFrom="children"
            parentPage="/content/aem-site-template-tailwind/guide/manage"
            showDescription="true"
            showModificationDate="false"
            sortOrder="asc"
            tagsMatch="any"
          >
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                jcr:primaryType="nt:unstructured"
                offset="0"
                width="4"
              />
                            <phone
                jcr:primaryType="nt:unstructured"
                offset="0"
                width="12"
              />
                        </cq:responsive>
                    </list>
                </content>
            </main>
        </root>
    </jcr:content>
    <assets />
    <site-elements />
    <authoring />
    <create-a-page />
    <publish />
</jcr:root>
