<?xml version="1.0" encoding="UTF-8" ?>
<jcr:root
    xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    xmlns:cq="http://www.day.com/jcr/cq/1.0"
    xmlns:jcr="http://www.jcp.org/jcr/1.0"
    xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page"
>
    <jcr:content
        cq:allowedTemplates="/conf/aem-site-template-tailwind/settings/wcm/templates/(?!xf-).*"
        cq:conf="/conf/aem-site-template-tailwind"
        cq:redirectTarget="/content/aem-site-template-tailwind/en"
        cq:template="/conf/aem-site-template-tailwind/settings/wcm/templates/blank-page"
        jcr:primaryType="cq:PageContent"
        jcr:title="aem-site-template-tailwind"
        sling:configRef="/conf/aem-site-template-tailwind"
        sling:redirect="true"
        sling:redirectStatus="{Long}302"
        sling:resourceType="core/wcm/components/page/v2/page"
        brandSlug="EZ-AEM Tailwind AEM Site Template"
        brandSlug_override="true"
        pwaCachestrategy="staleWhileRevalidate"
        pwaDisplay="standalone"
        pwaOrientation="any"
    >
        <image jcr:primaryType="nt:unstructured">
            <file />
        </image>
    </jcr:content>
    <readme />
    <en />
    <de />
    <fr />
</jcr:root>
