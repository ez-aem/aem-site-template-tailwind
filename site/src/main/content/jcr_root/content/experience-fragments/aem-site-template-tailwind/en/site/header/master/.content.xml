<?xml version="1.0" encoding="UTF-8" ?>
<jcr:root
    xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    xmlns:cq="http://www.day.com/jcr/cq/1.0"
    xmlns:jcr="http://www.jcp.org/jcr/1.0"
    xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page"
>
    <jcr:content
        cq:tags="[]"
        cq:template="/conf/aem-site-template-tailwind/settings/wcm/templates/xf-web-variation"
        cq:xfMasterVariation="{Boolean}true"
        cq:xfVariantType="web"
        jcr:primaryType="cq:PageContent"
        jcr:title="Header"
        sling:resourceType="cq/experience-fragments/components/xfpage"
    >
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="core/wcm/components/container/v1/container"
            id="main-header-wrapper"
            layout="responsiveGrid"
        >
            <container_1356041374
                cq:styleIds="[container-grid-width]"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="core/wcm/components/container/v1/container"
                id="main-header"
                layout="responsiveGrid"
            >
                <image_271092066
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core/wcm/components/image/v3/image"
                    alt="Wireframe"
                    altValueFromDAM="false"
                    displayPopupTitle="false"
                    fileReference="/content/dam/aem-site-template-tailwind/Group 3458@2x.png"
                    linkURL="/content/aem-site-template-tailwind/en/home"
                    titleValueFromDAM="true"
                >
                    <cq:responsive jcr:primaryType="nt:unstructured">
                        <default
                            jcr:primaryType="nt:unstructured"
                            offset="0"
                            width="2"
                        />
                    </cq:responsive>
                </image_271092066>
                <button
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core/wcm/components/button/v2/button"
                    accessibilityLabel="Toggle Main Menu"
                    id="mobile-header-button"
                >
                    <cq:responsive jcr:primaryType="nt:unstructured">
                        <default
                            jcr:primaryType="nt:unstructured"
                            behavior="hide"
                        />
                    </cq:responsive>
                </button>
                <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core/wcm/components/container/v1/container"
                    id="mobile-header"
                    layout="responsiveGrid"
                >
                    <cq:responsive jcr:primaryType="nt:unstructured">
                        <default
                            jcr:primaryType="nt:unstructured"
                            offset="0"
                            width="10"
                        />
                        <phone
                            jcr:primaryType="nt:unstructured"
                            offset="0"
                            width="12"
                        />
                    </cq:responsive>
                    <navigation
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core/wcm/components/navigation/v2/navigation"
                        collectAllPages="true"
                        disableShadowing="false"
                        navigationRoot="/content/aem-site-template-tailwind/en"
                        structureDepth="1"
                        structureStart="1"
                    >
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="6"
                            />
                            <phone
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="12"
                            />
                        </cq:responsive>
                    </navigation>
                    <search
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core/wcm/components/search/v1/search"
                    >
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="2"
                            />
                            <phone
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="12"
                            />
                        </cq:responsive>
                    </search>
                    <languagenavigation
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="core/wcm/components/languagenavigation/v2/languagenavigation"
                        navigationRoot="/content/aem-site-template-tailwind"
                        structureDepth="1"
                    >
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="2"
                            />
                            <phone
                                jcr:primaryType="nt:unstructured"
                                offset="0"
                                width="12"
                            />
                        </cq:responsive>
                    </languagenavigation>
                </container>
            </container_1356041374>
        </root>
    </jcr:content>
</jcr:root>
